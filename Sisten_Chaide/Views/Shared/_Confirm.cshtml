@using Sisten_Chaide.Resources.Labels
<div id="ConfirmBox" class="modal fade">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
				<h4 class="modal-title">@Labels.ConfirmTitle</h4>
			</div>
			<div class="modal-body">
				<p></p>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default" data-dismiss="modal">@Labels.Cancel</button>
				<button type="button" class="btn btn-primary">@Labels.Accept</button>
			</div>
		</div>
	</div>
</div>